{
  "name": "@org/inventory-api-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@org/inventory-api"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/inventory-api-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@org/inventory-api:build",
          "@org/inventory-api:serve"
        ]
      }
    }
  }
}
